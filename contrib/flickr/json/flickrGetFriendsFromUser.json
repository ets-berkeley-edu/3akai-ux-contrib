{
    "sling:resourceType": "sakai/proxy",
    "sakai:request-proxy-endpoint": 'http://api.flickr.com/services/rest/?method=flickr.contacts.getPublicList&api_key=${api_key}&user_id=${userid}&page=1&per_page=15&format=json&nojsoncallback=1',
    "sakai:request-proxy-method": "GET",
    "sakai:title": "flickr- Get the public contacts from a user",
    "sakai:shortDescription": "Get the contacts form a person",
    "sakai:description": [
        "This proxy will get the public contacts from a user"
    ],
     "sakai:parameters" : [
        "{\"name\" : \"userid\", \"description\" : \"(Required)The user id\"}",
        "{\"name\" : \"api_key\", \"description\" : \"(Required)The api key\"}",
    ]
}
